curl   -X POST --data-binary "@access.log" https://goaccess.suschkov.ru/generate_report;


curl -k  -X POST --data-binary "@access.log" https://goaccess.suschkov.test/generate_report;

curl   -X POST --data-binary "@access.log" http://127.0.0.1:3050;


ls ./access.log | awk '{print}' | xargs -I {} curl  -k -X POST --data-binary "@{}" https://goaccess.suschkov.test/generate_report


ls ./access.log | awk '{print}' | xargs -I {} curl  -X POST --data-binary "@{}" https://goaccess.suschkov.ru/generate_report   

curl  -X POST -T ./ https://goaccess.suschkov.ru/generate_report -o /pgrs && cat $_ && rm $_ 


curl -k  -X POST -T ./ https://goaccess.suschkov.test/v1/report -o ./pgrs  && tail -n 1 $_






